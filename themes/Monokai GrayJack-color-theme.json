{
	"name": "Monokai GrayJack",
	"type": "dark",
	"colors": {
		"activityBar.background": "#363338",
		"activityBar.activeBackground": "#716A60",
		"editor.background": "#242424",
		"editor.foreground": "#F9F5DD",
		"statusBar.background": "#363338",
		"activityBarBadge.background": "#5596F1",
		"sideBarTitle.foreground": "#bbbbbb",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"builtinType": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"typeAlias": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"typeParameter": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"interface": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"unresolvedReference": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"struct": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"union": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"enum": {
			"foreground": "#66CCE1",
			"fontStyle": "italic",
		},
		"enumMember": {
			"fontStyle": "underline",
			"foreground": "#5596F1"
		},
		"macro": {
			"foreground": "#5596F1"
		},
		"namespace": {
			"foreground": "#F9F5DD"
		},
		"property": {
			"foreground": "#CC8D66"
		},
		"attribute": {
			"foreground": "#FAF7B1"
		},
		"formatSpecifier": {
			"foreground": "#CC8D66"
		},
		"lifetime": {
			"foreground": "#CC8D66"
		},
		"variable.mutable": {
			"fontStyle": "underline",
			"foreground": "#FAA23D"
		},
		"parameter": {
			"fontStyle": "italic",
			"foreground": "#FAA23D"
		},
		"variable.constant": {
			"foreground": "#AA7DFC"
		},
		"number": {
			"foreground": "#AA7DFC"
		},
		"*.unsafe": {
			"foreground": "#FA8282"
		}
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#716A60"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"punctuation"
			],
			"settings": {
				"foreground": "#F9F5DD"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FA8282"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF3574"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"punctuation.symbol.match-branch",
				"punctuation.symbol.return-type",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FF3574"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#FF3574"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#B2E64C"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF3574"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"variable.other.constant",
				// "keyword.other.unit",
				// "keyword.other"
			],
			"settings": {
				"foreground": "#AA7DFC"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#E6DB7A"
			}
		},
		{
			"name": "Unsafe",
			"scope": [
				"keyword.other.unsafe",
			],
			"settings": {
				"foreground": "#FA8282",
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"meta.attributes",
			],
			"settings": {
				"foreground": "#FAF7B1",
			}
		},
		{
			"name": "Fields",
			"scope": [
				"variable.other.property",
			],
			"settings": {
				"foreground": "#CC8D66",
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
